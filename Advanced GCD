#include<bits/stdc++.h>
using namespace std;
int gcd(int a,int b){
  if(b==0)return a;
  return gcd(b,a%b);
}
int main(){
    
    int t;cin>>t;
    while(t--){
       string a;int b;
       cin>>b>>a;
    int bb=0;
    if(b==0){
      cout << a << " ";
      continue;
    }
    for(int i=0;i<a.length();i++){
      bb=(10*bb + a[i]-'0')%b;
    }
    cout<<gcd(bb,b)<<endl;
    }
    return 0;
}
