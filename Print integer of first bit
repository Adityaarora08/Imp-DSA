#include<bits/stdc++.h>
using namespace std;
int main(){
    
    // write your code here
    int t;
    cin>>t;
    while(t--){
        int n;cin>>n;
        int i=0;
        int a=1<<i;
        // cout << a << endl;
        while(i<n/2+1){
          a = 1 << i;
        //   cout << a << endl;
          if ((n & a) > 0) {
            cout << a << endl;
            break;
          }
            i++;
        }
    }
    return 0;
}
